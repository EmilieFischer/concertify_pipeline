{
    "compilerOptions": {
        "target": "es5",
        "lib": ["dom", "dom.iterable", "esnext"],
        "allowJs": true,
        "skipLibCheck": true,
        "strict": true,
        "noEmit": true,
        "esModuleInterop": true,
        "module": "esnext",
        "moduleResolution": "bundler",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "jsx": "preserve",
        "incremental": true,
        "plugins": [
            {
                "name": "next"
            }
        ],
        "paths": {
            "@/*": ["./src/*"]
        }
    },
    "include": [
        "next-env.d.ts",
        "**/*.ts",
        "**/*.tsx",
        ".next/types/**/*.ts",
        "src/app/verifyemail",
        "src/app/components/topNav/page.tsx",
        "src/app/component/page.tsx",
        "src/app/showVenues/[id]/page.tsx",
        "src/app/components/searchBar/page.jsx",
        "src/app/components/switchTheme/page.tsx",
        "src/app/components/search/page.jsx",
        "src/app/components/searchBar/page.jsx",
        "src/app/components/searchResults/page.jsx",
        "src/app/components/searchResultsList/page.jsx"
    ],
    "exclude": ["node_modules"]
}
